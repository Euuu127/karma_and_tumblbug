<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.karma_and_tumblbug.p1.project.ProjectDAO">

<select id="getProject" parameterType="ProjectDTO" resultType="ProjectDTO">
select * from project where id=#{id}
</select>

<select id="getProjectMedia" parameterType="ProjectDTO" resultType="ProjectDTO">
select * from project_media where id=#{id}
</select>

</mapper>